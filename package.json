{
  "name": "pand4",
  "version": "1.0.0",
  "description": "Pand4 - A modern, modular and responsive SCSS framework",
  "main": "./src/styles/pand4/pand4.scss",
  "type": "module",
  "exports": {
    ".": "./src/styles/pand4/pand4.scss",
    "./scss": "./src/styles/pand4/pand4.scss",
    "./css": "./src/styles/pand4/pand4.css",
    "./abstract": "./src/styles/pand4/abstract/_index.scss"
  },
  "files": [
    "src/styles/pand4/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "compile-css": "sass ./src/styles/pand4/pand4.scss ./src/styles/pand4/pand4.css",
    "prepublishOnly": "npm run compile-css"
  },
  "keywords": [
    "css",
    "scss",
    "sass",
    "framework",
    "responsive",
    "pand4",
    "frontend",
    "ui",
    "design-system"
  ],
  "author": "bascoy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bascoy/pand4.git"
  },
  "homepage": "https://pand4.vercel.app",
  "bugs": {
    "url": "https://github.com/bascoy/pand4/issues"
  },
  "dependencies": {
    "@astrojs/vercel": "^8.2.11",
    "astro": "^5.14.8"
  },
  "devDependencies": {
    "sass": "^1.93.2"
  },
  "peerDependencies": {
    "sass": "^1.50.0"
  }
}
